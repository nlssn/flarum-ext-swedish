{
    "name": "nlssn/flarum-ext-swedish",
    "type": "flarum-extension",
    "description": "Swedish language pack.",
    "keywords": ["locale"],
    "license": "MIT",
    "authors": [
        {
            "name": "Johannes Nilsson",
            "email": "johannes@nilsson.im"
        }
    ],
    "support": {
        "issues": "https://github.com/nlssn/flarum-ext-swedish/issues",
        "source": "https://github.com/nlssn/flarum-ext-swedish"
    },
    "require": {
        "flarum/core": "^0.1.0-beta.6"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        },
        "flarum-extension": {
            "title": "Swedish",
            "icon": {
                "image": "icon.svg",
                "backgroundColor": "#00247d",
                "backgroundSize": "cover",
                "backgroundPosition": "center"
            }
        },
        "flarum-locale": {
            "code": "sv",
            "title": "Swedish"
        }
    }
}
